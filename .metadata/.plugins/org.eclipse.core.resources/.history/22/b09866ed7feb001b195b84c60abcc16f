import java.util.Scanner;

public class Single {

	public static void findSingle(int arr[]) {

	}

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int arr[] = new int[10];

		System.out.print("Enter Array : ");
		for (int i = 0; i < 10; i++)
			arr[i] = sc.nextInt();
		findSingle(arr);
	}

}
