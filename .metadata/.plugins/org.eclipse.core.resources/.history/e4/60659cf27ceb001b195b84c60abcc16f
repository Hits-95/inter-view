import java.util.Scanner;

public class EvenPair {

	public static boolean isEven(int num) {
		return (num % 2 == 0);
	}
	// if(num % 2 == 0)
	// return true;
	// else
	// return false;

	public static void main(String[] args) {

		int[] arr = new int[10];
		Scanner sc = new Scanner(System.in);

		System.out.print("Enter Array : ");
		for (int i = 0; i < 10; i++)
			arr[i] = sc.nextInt();

	}

}
